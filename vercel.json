{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "npm run build",
  "devCommand": "npm run start:dev",
  "builds": [
    {
      "use": "@vercel/node",
      "src": "dist/src/main.js"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
